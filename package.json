{
  "name": "barber-api",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "lint": "eslint src --ext js,ts",
    "dev:http": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --no-notify --respawn src/main/server.ts | pino-pretty --messageKey message --ignore severity",
    "create-controller": "node libs/createDynamicFiles/CreateController/createController.ts",
    "dev:worker": "PROCESS_NAME=worker tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --no-notify --respawn src/main/worker.ts | pino-pretty --messageKey message --ignore severity",
    "dev:docs": "redoc-cli serve docs/api.yml --watch --options.noAutoAuth --options.hideDownloadButton",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "prepush": "yarn test",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn test --findRelatedTests",
    "build": "yarn build:app",
    "build:app": "tsc && tsc-alias",
    "build:docs": "redoc-cli bundle docs/api.yml -o dist/docs/index.html --options.noAutoAuth --options.hideDownloadButton"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@eslint/eslintrc": "^3.0.2",
    "@jest-mock/express": "^2.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^15.2.2",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "redoc-cli": "^0.13.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.9.0",
    "@prisma/client": "^5.13.0",
    "@supabase/supabase-js": "^2.43.2",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "env-var": "^7.4.1",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.20.0",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.1.0"
  }
}
